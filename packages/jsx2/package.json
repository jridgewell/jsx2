{
  "name": "jsx2",
  "version": "0.0.0",
  "description": "Just your normal (JSX) syntax, with faster performance and less memory",
  "keywords": [
    "jsx"
  ],
  "author": "Justin Ridgewell <justin@ridgewell.name>",
  "homepage": "",
  "license": "MIT",
  "main": "dist/jsx2.umd.js",
  "module": "dist/jsx2.mjs",
  "typings": "dist/types/jsx2.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/jridgewell/jsx2/tree/master/packages/jsx2",
  "scripts": {
    "build": "run-s -n build:*",
    "build:rollup": "rollup -c rollup.config.ts",
    "lint": "run-s -n lint:*",
    "lint:prettier": "npm run test:lint:prettier -- --write",
    "lint:ts": "npm run test:lint:ts -- --fix",
    "prebuild": "rm -rf dist",
    "test": "run-s -n test:lint 'test:only -- --no-cache'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:lint": "run-s -n test:lint:*",
    "test:lint:prettier": "prettier --check '{src,test}/**/*.ts'",
    "test:lint:ts": "eslint '{src,test}/**/*.ts'",
    "test:only": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "preversion": "run-s test build",
    "prepublishOnly": "npm run preversion"
  },
  "devDependencies": {
    "@jridgewell/path-sorter": "0.0.1",
    "@rollup/plugin-strip": "2.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "@types/jest": "26.0.15",
    "@types/jsdom": "16.2.5",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-local-rules": "0.1.1",
    "jest": "26.6.1",
    "jest-config": "26.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "rollup": "2.32.1",
    "ts-jest": "26.4.2",
    "typescript": "4.0.3"
  }
}
